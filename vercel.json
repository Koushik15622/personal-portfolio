{
    "version": 2,
    "builds": [
      {
        "src": "build/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      },
      {
        "src": "/resume",
        "dest": "/backend/server.js"
      }
    ]
  }
  